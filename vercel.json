{
  "version": 2,
  "name": "atividade-multidisciplinar",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index-acesso-rapido.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": true
  }
}
